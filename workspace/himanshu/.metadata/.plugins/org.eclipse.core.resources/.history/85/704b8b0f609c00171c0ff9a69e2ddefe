
public class Test1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		for(int i = 1; i < 3; i++)
		  for(int j = 3; j > i; j--)
		     assert (i!=j && i < j) : i; 
		     
		 	char c;
			int i;
			c = 'A';		// 1
			i = c;		//2
			System.out.println(i + "    "+c);
			//c = i + 1;	//3
			c++;		//4
			System.out.println(i + "    "+c);
			i=c;
			System.out.println(i + "    "+c);
			
			
			  int i;
		        do {
		            i++;
		        }
		        while(i < 0);
		        System.out.println(i);
	
	}

}
